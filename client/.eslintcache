AIL ADDRESS'\n                    name='email'\n                    value={email}\n                    onChange={onChange}\n                />\n                <button onClick={onSubmit} class='button is-white has-text-primary'>Register</button>\n                <p class={registerMsg ? 'title is-size-2' : 'displayNone'}>{registerMsg}</p>\n                <p class={registerMsg ? 'subtitle is size-4' : 'displayNone'}> stay or <a href='/home'>redirect to home? </a></p>\n            </div>\n        </div>\n    )\n}","/Users/carolinepark/Desktop/ToDoList/client/src/components/screens/SignIn.js",["110","111","112","113","114"],"/Users/carolinepark/Desktop/ToDoList/client/src/services/auth.js",[],"/Users/carolinepark/Desktop/ToDoList/client/src/services/apiConfig.js",[],"/Users/carolinepark/Desktop/ToDoList/client/src/components/container.js",["115"],"import React from 'react'\nimport { Routes } from './routes/index'\n\n\n\n\nexport default class Container extends React.Component {\n    constructor(props) {\n        super(props)\n        this.state = {\n            user: null,\n            toDos: [],\n            landingDisplay: true,\n            todoDisplay: false\n        }\n    }\n\n\n    setUser = user => this.setState({ user })\n\n    clearUser = () => this.setState({ user: null })\n\n    toggleLanding = () => {\n        this.setState({\n            landingDisplay: true,\n            todoDisplay: false\n        })\n    }\n\n    toggleToDo = () => {\n        this.setState({\n            todoDisplay: true,\n            landingDisplay: false\n        })\n    }\n\n\n\n    render() {\n\n        const { user, toDos } = this.state\n        return (\n\n        \n        <>\n        <Routes\n        setUser={this.setUser}\n        user={user}\n        clearUser={this.clearUser}\n        />\n        </>\n        )\n    }\n}","/Users/carolinepark/Desktop/ToDoList/client/src/components/Shared/Navbar.js",["116","117"],"import React from 'react'\nimport AppBar from '@material-ui/c